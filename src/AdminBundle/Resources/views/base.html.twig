{% extends 'base.html.twig' %}

{#head#}
{% block title %}
    SB Admin 2 - Bootstrap Admin Theme
{% endblock %} {#block title#}

{% block stylesheets %}
    <!-- Bootstrap -->
    {{ parent() }}

    <!-- MetisMenu CSS -->
    <link href="{{ asset("assets/sb-admin-2/vendor/metisMenu/metisMenu.css") }}" rel="stylesheet">

    <!-- Custom CSS -->
    <link href="{{ asset("assets/sb-admin-2/dist/css/sb-admin-2.css") }}" rel="stylesheet">

    <!-- Custom Fonts -->
    <link href="{{ asset("assets/sb-admin-2/vendor/font-awesome/css/font-awesome.min.css") }}" rel="stylesheet">
{% endblock %} {#block style#}

{#body#}
{% block body %}
    {#body navbar#}
    {% block body_navbar %}
        {{ include('@Admin/body-navbar.html.twig', {}) }}
    {% endblock %} {#block navbar#}

    {#body page-content#}
    {% block body_page_content %}
        {{ include('@Admin/body-page-content.html.twig', {'defaultPageContent': 'This is page content'}) }}
    {% endblock %} {#block body_page_content#}

    {#body javascripts#}
    {% block body_javascripts %}
        <!-- Bootstrap -->
        {{ parent() }}

        <!-- Metis Menu Plugin JavaScript -->
        <script src="{{ asset("assets/sb-admin-2/vendor/metisMenu/metisMenu.js") }}"></script>

        <!-- Custom Theme JavaScript -->
        <script src="{{ asset("assets/sb-admin-2/dist/js/sb-admin-2.js") }}"></script>
    {% endblock %} {#block body_javascripts#}

{% endblock %} {#end block body#}
